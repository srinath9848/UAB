@model UAB.DTO.ChartSummaryDTO
<style type="text/css">
    .col-md-3 {
        margin-left: 10px !important;
        max-width: 20% !important;
    }

    .card .card-title {
        margin-bottom: 0px !important;
    }

    .form-control {
        padding: 0 !important;
    }

    .col-sm-2 {
        max-width: 6.66667% !important;
    }

    .col-sm-3 {
        max-width: 20% !important;
    }
</style>

<div class="col-md-12 form-horizontal">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">@Model.ProjectName</h4><br />
            <div class="form-group row">
                <label for="fname" class="col-sm-2">MRN</label>
                <div class="col-sm-3">
                    @Html.TextBoxFor(model => model.CodingDTO.PatientMRN, new { @class = "form-control", @readonly = "readonly" })
                </div>
                <label for="fname" class="col-sm-2">Patient</label>
                <div class="col-sm-3">
                    @Html.TextBoxFor(model => model.CodingDTO.Name, new { @class = "form-control", @readonly = "readonly" })
                </div>
                <label for="fname" class="col-sm-2">DOS</label>
                <div class="col-sm-3">
                    @Html.TextBoxFor(model => model.CodingDTO.DateOfService, new { @class = "form-control", @readonly = "readonly" })
                </div>
                <button type="button" class="btn btn-link Available" onclick="ViewHistory()">View History</button>
            </div>
        </div>
    </div>
</div>


<script src="~/dashboardassets/libs/jquery/dist/jquery.min.js"></script>
<script src="~/dashboardassets/libs/jquery-validation/dist/jquery.validate.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validation-unobtrusive/3.2.11/jquery.validate.unobtrusive.min.js" type="text/javascript"></script>
<script src="~/dashboardassets/extra-libs/DataTables/datatables.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        ViewHistory = function () {

            $.ajax({
                type: "GET",
                url: "@Url.Action("ViewHistory", "UAB")",
                //data: {"ccid": $("#ccid").val()},
                success: function (response) {
                    $("#modalContent").html(response);
                },
                failure: function (response) {
                    alert(response);
                }
            });
        };
});
</script>

