<script src="~/dashboardassets/libs/jquery-validation/dist/jquery.validate.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validation-unobtrusive/3.2.11/jquery.validate.unobtrusive.min.js" type="text/javascript"></script>

<script type="text/javascript">
    $("#PayorID").focus();
    $("#hdnProviderPosted").val('');
     var enableNext = @(ViewBag.EnableNext == null ? "false": ViewBag.EnableNext);
    var enablePrevious = @(ViewBag.EnablePrevious == null ? "false" : ViewBag.EnablePrevious);

    var isBlocked = @(ViewBag.IsBlocked == null ? "0": "1");
    if (isBlocked == 1) {
        $("#btnBlockHistory").show();
        $("#btnProviderPosted").hide();

        if (enablePrevious == true) {
            $("#btnPrevious").css({
                'opacity': '100', 'pointer-events': 'auto'
            });
        } else {
            $("#btnPrevious").css({
                'opacity': '0.65', 'pointer-events': 'none'
            });
        }
        if (enableNext == true) {
            $("#btnNext").css({
                'opacity': '100', 'pointer-events': 'auto'
            });
        } else {
            $("#btnNext").css({
                'opacity': '0.65', 'pointer-events': 'none'
            });
        }
    }
    else {
        $("#btnPrevious").hide();
        $("#btnNext").hide();
    }


    $(document).ready(function () {

        $("#hdnAuditProviders").val("@Model.auditDTO?.ProviderIDs");
        $("#hdnAuditCPTCodes").val("@Model.auditDTO?.CPTCodes");

        ProviderPostedClinicalcase = function () {
            $("#_providerPostedId").val($("#providerPostedId").val());
            $("#_payorPostedId").val($("#payorPostedId").val());
            $("#_txtPostingDate").val($("#txtPostingDate").val());
            $("#_txtCoderComment").val($("#txtCoderComment").val());

            $("#frmSubmitCodingAvailableChart").submit();
        }
        ModalClose1 = function () {
            $("#modalContent").html('');
       }

       EnableProviderDrpDwn = function () {

           if ($("#chkWrongProvider").is(":checked")) {
               $("#hdnWrongProvider").val("true");
               $("#ProviderID").css("pointer-events", "auto");
               $("#ProviderID").css("background-color", "");
               $("#ProviderID").prop("selectedIndex", 0).val();
               EnableButton();
           }
           else {
               $("#hdnWrongProvider").val("false");
               $("#ProviderID").css("pointer-events", "none");
               $("#ProviderID").css("background-color", "#e9ecef");
               $("#ProviderID").val($("#InitialProviderID").val());
               EnableButton();
           }
       }
});

    function ShowSubmitPopup(buttonType,isAduditRequired)
    {
        $.ajax({
            type: "GET",
            url: "@Url.Action("codingSubmitPopup", "UAB")",
            data: {
                "chartSummaryDTO": "@Model",
                "buttonType": buttonType,
                "isAuditRequired": isAduditRequired
            },
            success: function (response) {
                $("#modalContent").html(response);
            },
            failure: function (response) {
                alert(response);
            }
        });
    }


    $('#codingSubmit,#codingSubmitAndGetNext,#btnProviderPosted').click(function (e) {

        var arrProviders = [];
        var arrCPTCodes = [];

        if ($("#hdnAuditProviders").val() != "")
            arrProviders = $("#hdnAuditProviders").val().split(",");

        if ($("#hdnAuditCPTCodes").val() != "")
            arrCPTCodes = $("#hdnAuditCPTCodes").val().split(",");

        var auditMandatory = "";

        if (arrProviders.length !=0 && jQuery.inArray($("#ProviderID").val(), arrProviders) != -1)
            auditMandatory = "1";

        var claimArray = [];
        $(".AlignCross .tab").each(function () {
            var tabid = $(this).attr('id');
            if (arrProviders.length != 0 && jQuery.inArray($("#Providers_" + tabid).val(), arrProviders) != -1)
                auditMandatory = "1";
            var billingProviderID = 0;
            if ($("#BillingProviders_" + tabid).length)
                billingProviderID = $("#BillingProviders_" + tabid).val();

            claimArray.push($("#Providers_" + tabid).val() + "^" + billingProviderID + "^" + $("#Payors_" + tabid).val() + "^" + $("#NoteTitle_" + tabid).val() + "^" + $("#ProviderFeedbacks_" + tabid).val());
            $("#hdnClaim" + tabid).val(claimArray);
            claimArray = [];
        });

        //Claim1
        var DxCodes = $("[id*=txtDx_]");
        $("#hdnDxCodes").val("");
        for (var i = 0; i < DxCodes.length; i++) {

                if ($("#" + $(DxCodes[i]).attr("id")).val() != "") {
                    if ($("#hdnDxCodes").val() == "")
                        $("#hdnDxCodes").val($(DxCodes[i]).val());
                    else
                        $("#hdnDxCodes").val($("#hdnDxCodes").val() + "," + $(DxCodes[i]).val());
                }
        }

        //Claim2
        DxCodes = $("[id*=txtDx2Claim_]");
        $("#hdnDxCodes1").val("");
        for (var i = 0; i < DxCodes.length; i++) {

            if ($("#" + $(DxCodes[i]).attr("id")).val() != "") {
                if ($("#hdnDxCodes1").val() == "")
                    $("#hdnDxCodes1").val($(DxCodes[i]).val());
                else
                    $("#hdnDxCodes1").val($("#hdnDxCodes1").val() + "," + $(DxCodes[i]).val());
            }
        }

        //Claim3
        DxCodes = $("[id*=txtDx3Claim_]");
        $("#hdnDxCodes2").val("");
        for (var i = 0; i < DxCodes.length; i++) {

            if ($("#" + $(DxCodes[i]).attr("id")).val() != "") {
                if ($("#hdnDxCodes2").val() == "")
                    $("#hdnDxCodes2").val($(DxCodes[i]).val());
                else
                    $("#hdnDxCodes2").val($("#hdnDxCodes2").val() + "," + $(DxCodes[i]).val());
            }
        }

        //Claim4
        DxCodes = $("[id*=txtDx4Claim_]");
        $("#hdnDxCodes3").val("");
        for (var i = 0; i < DxCodes.length; i++) {

            if ($("#" + $(DxCodes[i]).attr("id")).val() != "") {
                if ($("#hdnDxCodes3").val() == "")
                    $("#hdnDxCodes3").val($(DxCodes[i]).val());
                else
                    $("#hdnDxCodes3").val($("#hdnDxCodes3").val() + "," + $(DxCodes[i]).val());
            }
        }

        //Default CPT
        $("#hdnCptCodes").val("");
        var CptCodes = $("[id*=DivCptRow_]");
        for (var i = 1; i <= CptCodes.length; i++) {

            if (arrCPTCodes.length != 0 && jQuery.inArray($("#txtCpt_" + i).val(), arrCPTCodes) != -1)
                auditMandatory = "1";

            if ($("#hdnCptCodes").val() == "") {
                if ($("#txtCpt_" + i).val() != "") {
                    var mod = $("#txtMod_" + i).val() != "" ? $("#txtMod_" + i).val() : null;
                    var qty = $("#txtQty_" + i).val() != "" ? $("#txtQty_" + i).val() : null;
                    var links = $("#txtLinks_" + i).val() != "" ? $("#txtLinks_" + i).val() : null;
                    $("#hdnCptCodes").val($("#txtCpt_" + i).val() + "^" + mod + "^" + qty + "^" + links);
                }
            } else {
                if ($("#txtCpt_" + i).val() != "") {
                    var mod = $("#txtMod_" + i).val() != "" ? $("#txtMod_" + i).val() : null;
                    var qty = $("#txtQty_" + i).val() != "" ? $("#txtQty_" + i).val() : null;
                    var links = $("#txtLinks_" + i).val() != "" ? $("#txtLinks_" + i).val() : null;
                    $("#hdnCptCodes").val($("#hdnCptCodes").val() + "|" + $("#txtCpt_" + i).val() + "^" + mod + "^" + qty + "^" + links);
               }
            }
        }

        //CPT2 to CPT4
        for (var j = 2; j <= 4; j++)
        {
            $("#hdnCptCodes" + (j-1)).val("");

            var CptClaimCodes = $("[id*=Div" + j + "CptRow_]");


            for (var i = 1; i <= CptClaimCodes.length; i++) {

                if (arrCPTCodes.length != 0 && jQuery.inArray($("#txt" + j + "Cpt_" + i).val(), arrCPTCodes) != -1)
                    auditMandatory = "1";

                if ($("#hdnCptCodes" + (j-1)).val() == "") {
                    if ($("#txt"+ j +"Cpt_" + i).val() != "") {
                        var mod = $("#txt" + j + "mod_" + i).val() != "" ? $("#txt" + j +"mod_" + i).val() : null;
                        var qty = $("#txt" + j + "qty_" + i).val() != "" ? $("#txt" + j +"qty_" + i).val() : null;
                        var links = $("#txt" + j + "links_" + i).val() != "" ? $("#txt" + j +"links_" + i).val() : null;
                        $("#hdnCptCodes" + (j-1)).val($("#txt" + j +"Cpt_" + i).val() + "^" + mod + "^" + qty + "^" + links);
                    }
                } else {
                    if ($("#txt" + j +"Cpt_" + i).val() != "") {
                        var mod = $("#txt" + j + "mod_" + i).val() != "" ? $("#txt" + j +"mod_" + i).val() : null;
                        var qty = $("#txt" + j + "qty_" + i).val() != "" ? $("#txt" + j +"qty_" + i).val() : null;
                        var links = $("#txt" + j + "links_" + i).val() != "" ? $("#txt" + j +"links_" + i).val() : null;
                        $("#hdnCptCodes" + (j-1)).val($("#hdnCptCodes" + (j-1)).val() + "|" + $("#txt" + j +"Cpt_" + i).val() + "^" + mod + "^" + qty + "^" + links);
                    }
                }
            }
        }

        var buttonType = $(this).attr("name");
        $("#hdnButtonType").val(buttonType);
        if (buttonType === "btnProviderPosted") {
            $("#hdnProviderPosted").val('1');
            $.ajax({
                         type: "GET",
                         url: "@Url.Action("ProviderPostedClinicalcase", "UAB")",
                         data: {
                             "chartSummaryDTO": "@Model",
                             "providerId": $("#providerId").val(),
                             "txtPostingDate": $("#txtPostingDate").val(),
                             "txtCoderComment": $("#txtCoderComment").val(),
                             "buttonType": buttonType
                         },
                         success: function (response) {
                             $("#modalContent").html(response);
                         },
                         failure: function (response) {
                             alert(response);
                         }
                     });
        }
        else {
            var isAuditRequired = (@Model.IsAuditRequired.ToString().ToLower());

            if (auditMandatory == "1") {
                ShowSubmitPopup('',"true");
            }
            else {
                ShowSubmitPopup('', isAuditRequired);
            }
        }
        EnableButton();
        EnableButtonForClaim2();
        EnableButtonForClaim3();
    });

    $("#coding").addClass('selected');
    $('.Approved').hide();
    $('#divAnswer').hide();

