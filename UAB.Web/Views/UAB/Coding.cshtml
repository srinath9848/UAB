            && $('#Providers_3').val() !== "" && $('#BillingProviders_3').val() !== "" && $('#Payors_3').val() !== "" && $('.Dxclaim_3').val() !== "" && $('#txt3Cpt_1').val() !== ""
            && $('#Providers_4').val() !== "" && $('#BillingProviders_4').val() !== "" && $('#Payors_4').val() !== "" && $('.Dxclaim_4').val() !== "" && $('#txt4Cpt_1').val() !== "") {
            $('#codingSubmit').prop('disabled', false);
            $('#codingSubmitAndGetNext').prop('disabled', false);
            $('#btnProviderPosted').prop('disabled', false);
        }
        else {
            $('#codingSubmit').prop('disabled', true);
            $('#codingSubmitAndGetNext').prop('disabled', true);
            $('#btnProviderPosted').prop('disabled', true);
        }
    }

    $(document).on('keyup keypress change', '.cptQtyClaim,.cptLinksClaim', function () {
        this.value = this.value.replace(/[^0-9\.]/g, '');
    });
</script>
