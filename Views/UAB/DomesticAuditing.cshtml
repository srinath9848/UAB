
@{
    ViewData["Title"] = "Initial QA - Domestic and Global QA";
}
<div class="container-fluid">
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">Chart Details</h4>
                </div>
                <partial name="_InitialData" />
            </div>

            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">Coding Details</h4>
                </div>
                <div class="card-body">
                    <div class="row mb-3">
                        <div class="col-lg-6">
                            <div class="form-group row">
                                <label class="col-md-3 m-t-15">
                                    Date Assigned
                                </label>
                                <div class="col-md-9 input-group">
                                    <input type="text" readonly class="form-control" id="datepicker-autoclose" placeholder="mm/dd/yyyy">
                                    <div class="input-group-append">
                                        <span class="input-group-text"><i class="fa fa-calendar"></i></span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div class="form-group row">
                                <label class="col-md-3 m-t-15">
                                    Payor
                                </label>
                                <div class="col-md-9">
                                    <select class="form-control" readonly>
                                        <option>MCAL</option>
                                        <option>Commercial</option>
                                        <option>SP</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row mb-3">
                        <div class="col-lg-6">
                            <div class="form-group row">
                                <label class="col-md-3 m-t-15">Note Title</label>
                                <div class="col-sm-9">
                                    <textarea class="form-control" readonly></textarea>
                                </div>
                            </div>
                        </div>

                        <div class="col-lg-6">
                            <div class="form-group row">
                                <label class="col-md-3 m-t-15">
                                    Provider
                                </label>
                                <div class="col-md-9">
                                    <select class="form-control" readonly>
                                        <option>Bush, Nathan, APP</option>
                                        <option>Dillon, Kayce, APP</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row mb-3">
                        <div class="col-lg-6">
                            <div class="form-group row">
                                <label class="col-md-3 m-t-15">
                                    CPT
                                </label>
                                <div class="col-md-9">
                                    <select class="form-control" readonly>
                                        <option>Question</option>
                                        <option>Answered</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div class="form-group row">
                                <label class="col-md-3 m-t-15">Mod</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" readonly id="Mod" placeholder="Mod">
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row mb-3">
                        <div class="col-lg-6">
                            <div class="form-group row">
                                <label class="col-md-3 m-t-15">Dx 1-4 (code followed by a comma and space)</label>
                                <div class="col-sm-9">
                                    <textarea class="form-control" readonly></textarea>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div class="form-group row">
                                <label class="col-md-3 m-t-15">
                                    Feedback to Provider
                                </label>
                                <div class="col-md-9">
                                    <select class="form-control" readonly>
                                        <option>Education: CC doesn't support vital organ failure/life threatening deteriortation</option>
                                        <option>Education: Consult not coded due to no requesting provider on file</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row mb-3">
                        <div class="col-lg-6">
                            <div class="form-group row">
                                <label class="col-md-3 m-t-15">Coder Questions</label>
                                <div class="col-sm-9">
                                    <textarea class="form-control" readonly></textarea>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="card">
                <div class="card-body">
                    <div class="col-12">
                        <div class="card-body">
                            <h4 class="card-title">Global QA Details</h4>
                        </div>
                        <div class="card-body">
                            <div class="col-12">
                                <div class="row mb-3">
                                    <div class="col-lg-6">
                                        <div class="form-group row">
                                            <label class="col-md-3 m-t-15">
                                                Incorrect / Correct
                                            </label>
                                            <div class="col-md-9">
                                                <select class="form-control" readonly>
                                                    <option>Correct</option>
                                                    <option>Incorrect</option>
                                                    <option>Educational</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="form-group row">
                                            <label class="col-md-3 m-t-15">Corrections/Education</label>
                                            <div class="col-sm-9">
                                                <textarea class="form-control" readonly></textarea>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row mb-3">
                                    <div class="col-lg-6">
                                        <div class="form-group row">
                                            <label class="col-md-3 m-t-15">
                                                Date Rvwd/ Corrected by Global QA/Coder
                                            </label>
                                            <div class="col-md-9 input-group">
                                                <input type="text" class="form-control" readonly id="datepicker-autoclose" placeholder="mm/dd/yyyy">
                                                <div class="input-group-append">
                                                    <span class="input-group-text"><i class="fa fa-calendar"></i></span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="form-group row">
                                            <label class="col-md-3 m-t-15">Coder Rebuttal</label>
                                            <div class="col-sm-9">
                                                <input type="text" class="form-control" readonly id="Mod">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row mb-3">
                                    <div class="col-lg-6">
                                        <div class="form-group row">
                                            <label class="col-md-3 m-t-15">
                                                Error Type (mgt use only)
                                            </label>
                                            <div class="col-md-9">
                                                <select class="form-control" readonly>
                                                    <option>CC not Supported</option>
                                                    <option>Consult not Supported</option>
                                                    <option>Mod Error</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">QA Details</h4>
                </div>
                <div class="card-body">
                    <div class="col-12">
                        <div class="row mb-3">
                            <div class="col-lg-6">
                                <div class="form-group row">
                                    <label class="col-md-3 m-t-15">
                                        Incorrect / Correct
                                    </label>
                                    <div class="col-md-9">
                                        <select class="form-control">
                                            <option>Correct</option>
                                            <option>Incorrect</option>
                                            <option>Educational</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="form-group row">
                                    <label class="col-md-3 m-t-15">Corrections/Education</label>
                                    <div class="col-sm-9">
                                        <textarea class="form-control"></textarea>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row mb-3">
                            <div class="col-lg-6">
                                <div class="form-group row">
                                    <label class="col-md-3 m-t-15">Coder/QA Rebuttals</label>
                                    <div class="col-sm-9">
                                        <input type="text" class="form-control" id="Mod">
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="form-group row">
                                    <label class="col-md-3 m-t-15">
                                        Date Rvwd/ Corrected by Global QA/Coder
                                    </label>
                                    <div class="col-md-9 input-group">
                                        <input type="text" class="form-control" id="datepicker-autoclose" placeholder="mm/dd/yyyy">
                                        <div class="input-group-append">
                                            <span class="input-group-text"><i class="fa fa-calendar"></i></span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row mb-3">
                            <div class="col-lg-6">
                                <div class="form-group row">
                                    <label class="col-md-3 m-t-15">
                                        Error Type (mgt use only)
                                    </label>
                                    <div class="col-md-9">
                                        <select class="form-control">
                                            <option>CC not Supported</option>
                                            <option>Consult not Supported</option>
                                            <option>Mod Error</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="form-group row">
                                    <label class="col-md-3 m-t-15">Notes from Jen</label>
                                    <div class="col-sm-9">
                                        <textarea class="form-control"></textarea>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row mb-3">
                            <div class="col-lg-6">
                                <div class="form-group row">
                                    <div class="custom-control custom-checkbox mr-sm-2">
                                        <input type="checkbox" class="custom-control-input" id="customControlAutosizing1">
                                        <label class="custom-control-label" for="customControlAutosizing1">Okay to Post in PK?</label>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="border-top">
                            <div class="card-body" style="text-align:right">
                                <button type="submit" class="btn btn-info">Save</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="~/dashboardassets/libs/jquery/dist/jquery.min.js"></script>
<script src="~/dashboardassets/libs/bootstrap-datepicker/dist/js/bootstrap-datepicker.min.js"></script>
<script type="text/javascript">
    /*datwpicker*/
    jQuery('.mydatepicker').datepicker();
    jQuery('#datepicker-autoclose').datepicker({
        autoclose: true,
        todayHighlight: true
    });
</script>
