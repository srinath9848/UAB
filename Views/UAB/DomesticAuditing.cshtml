
@{
    ViewData["Title"] = "Initial QA - Domestic and Global QA";
}
<div class="container-fluid">
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">QA Details</h4>
                </div>
                <div class="card-body">
                    <div class="col-12">
                        <div class="col-6">
                            <div class="form-group row" data-select2-id="12">
                                <label class="col-md-3 m-t-15">
                                    Incorrect / Correct
                                </label>
                                <div class="col-md-9" data-select2-id="11">
                                    <select class="select2 form-control custom-select select2-hidden-accessible" style="width: 100%; height:36px;" data-select2-id="1" tabindex="-1" aria-hidden="true">
                                        <option data-select2-id="3">Correct</option>
                                        <option data-select2-id="3">Incorrect</option>
                                        <option data-select2-id="3">Educational</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="col-6">
                            <div class="form-group row">
                                <label class="col-md-3 m-t-15">Corrections/Education</label>
                                <div class="col-sm-9">
                                    <textarea class="form-control"></textarea>
                                </div>
                            </div>
                        </div>
                        <div class="col-6">
                            <div class="form-group row" data-select2-id="12">
                                <label class="col-md-3 m-t-15">Coder/QA Rebuttals</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" id="Mod">
                                </div>
                            </div>
                        </div>
                        <div class="col-6">
                            <div class="form-group row" data-select2-id="12">
                                <label class="col-md-3 m-t-15">
                                    Date Rvwd/ Corrected by Global QA/Coder
                                </label>
                                <div class="col-md-9 input-group" data-select2-id="11">
                                    <input type="text" class="form-control" id="datepicker-autoclose" placeholder="mm/dd/yyyy">
                                    <div class="input-group-append">
                                        <span class="input-group-text"><i class="fa fa-calendar"></i></span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-6">
                            <div class="form-group row" data-select2-id="12">
                                <label class="col-md-3 m-t-15">
                                    Error Type (mgt use only)
                                </label>
                                <div class="col-md-9" data-select2-id="11">
                                    <select class="select2 form-control custom-select select2-hidden-accessible" style="width: 100%; height:36px;" data-select2-id="1" tabindex="-1" aria-hidden="true">
                                        <option data-select2-id="3">CC not Supported</option>
                                        <option data-select2-id="3">Consult not Supported</option>
                                        <option data-select2-id="3">Mod Error</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="col-6">
                            <div class="form-group row">
                                <label class="col-md-3 m-t-15">Notes from Jen</label>
                                <div class="col-sm-9">
                                    <textarea class="form-control"></textarea>
                                </div>
                            </div>
                        </div>
                        <div class="col-6">
                            <div class="form-group row">
                                <div class="custom-control custom-checkbox mr-sm-2">
                                    <input type="checkbox" class="custom-control-input" id="customControlAutosizing1">
                                    <label class="custom-control-label" for="customControlAutosizing1">Okay to Post in PK?</label>
                                </div>
                            </div>
                        </div>
                        <div class="border-top">
                            <div class="card-body">
                                <button type="submit" class="btn btn-success">Save</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="~/dashboardassets/libs/jquery/dist/jquery.min.js"></script>
<script src="~/dashboardassets/libs/bootstrap-datepicker/dist/js/bootstrap-datepicker.min.js"></script>
<script type="text/javascript">
    /*datwpicker*/
    jQuery('.mydatepicker').datepicker();
    jQuery('#datepicker-autoclose').datepicker({
        autoclose: true,
        todayHighlight: true
    });
</script>